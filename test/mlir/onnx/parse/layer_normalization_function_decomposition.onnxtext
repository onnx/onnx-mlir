// RUN: onnx-mlir --functions-to-decompose=LayerNormalization --EmitONNXBasic --printIR %s | FileCheck %s

// from onnx-mlir issue #2492
<
   ir_version: 8,
   opset_import: ["" : 17]
>
agraph (float[12,3,5] X, float[5] S) => (float[12,3,5] LN) {
   LN = LayerNormalization (X, S)
}
